{
  "compilerOptions": {
    "module": "es6",
    "target": "es6",
    "strict": true,
    "strictNullChecks": false,
    "noEmitHelpers": true,
    "sourceMap": false,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost",
      "es2015.promise"
    ],
    "moduleResolution": "node",
  },
  "include": [
    "./assets",
    "./src",
    "./engine"
  ],
  "exclude": [
    "node_modules"
  ]
}